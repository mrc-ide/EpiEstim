.First.lib <- function (lib, pkg){
#.initAdegenetClasses()
#.initAdegenetUtils()
    library.dynam("adegenet", pkg, lib)
    startup.txt <- "   ==========================\n    adegenet 1.3-3 is loaded\n   ==========================\n\n - to start, type '?adegenet'\n - to browse adegenet website, type 'adegenetWeb()'\n - to post questions/comments: adegenet-forum@lists.r-forge.r-project.org\n\n"

    packageStartupMessage(startup.txt)
}
